file = open('s_conameu.csv','r')
fileR = file.read()
fileL = fileR.split("\n")
sectors = {}
for i in range(1, len(fileL)-1):
	currentSector = fileL[i].split(",")[0].replace('"','')
	sectors[currentSector] = [] 
	file2 = open(str(i)+'conameu.csv','r')
	file2R = file2.read()
	file2L = fileR.split("\n")
	for j in range(1, len(file2L)-1):
		currentIndustry = file2L[j].split(",")[0].replace('"','')
		sectors[currentSector].append(currentIndustry)


sectors = [\
